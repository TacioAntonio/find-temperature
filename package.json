{
  "name": "find-temperature",
  "version": "1.1.0",
  "description": "A CLI to find temperature of a city.",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf bin",
    "build": "npm run clear && .\\node_modules\\.bin\\babel --out-dir bin src",
    "build:watch": "npm run build -- --watch",
    "lint": ".\\node_modules\\.bin\\eslint src\\*.js",
    "prepush": "npm run lint",
    "test": ".\\node_modules\\.bin\\mocha tests/**/*.spec.js --require babel-register --require babel-polyfill",
    "test:tdd": "npm run test -- --watch",
    "test:coverage": "nyc npm test"
  },
  "preferGlobal": true,
  "bin": {
    "find-temperature": "bin/main.js"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TacioAntonio/find-temperature.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "TacioAntonio <tacioantonio10@gmail.com> (https://tacioantonio.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TacioAntonio/find-temperature/issues"
  },
  "homepage": "https://github.com/TacioAntonio/find-temperature#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "child_process": "^1.0.2",
    "commander": "^2.10.0",
    "fetch": "^1.1.0",
    "ora": "^1.4.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.5"
  }
}
